package prog9_4;

public class Node {

	String data;
	Node next;
	
//	Node(String data){
//		this.data =data;
//	}
//	
	@Override
	public String toString() {
		 if(data == null) return "";
			StringBuilder sb = new StringBuilder(data + " ");
			sb = toString(sb, next);
			return sb.toString();
	}
	
	private StringBuilder toString (StringBuilder sb, Node node) {
		if(data == null) return sb;
		sb.append(node.data+" ");
		return toString(sb,node.next);
	}
}
